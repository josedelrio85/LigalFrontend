@model LigalFrontend.ViewModels.InspeccionesGpsVM
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Posicionamiento Inspecciones";
    var controlador = ViewContext.RouteData.Values["controller"].ToString();
}

@Html.AntiForgeryToken()
@{
    if (controlador.Contains("Quino"))
    {
        Html.RenderPartial("~/Views/Shared/Buscador/_FiltroInspeccionesQuinoGps.cshtml");
        ViewBag.Title = "Posicionamiento Inspecciones Quinolona";
    }
    else
    {
        Html.RenderPartial("~/Views/Shared/Buscador/_FiltroInspeccionesGps.cshtml");
    }
}

@section Scripts
{

<script type="text/javascript">
    google.maps.event.addDomListener(window, 'load', initialize);
</script> 
}

@{
    Html.RenderPartial("~/Views/Shared/DisplayTemplates/divTituloPosicionamiento.cshtml");
}


<div class="containerMapa">
    <div id="map" class="map-canvas"></div>
</div>


<div id="indexReal"></div>
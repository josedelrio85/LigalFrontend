@model LigalFrontend.ViewModels.RecogClientesVM
@using LigalFrontend.Helpers

@{ 
    Layout = null;
}

@{
    ViewBag.id = Model.recogidasR.ID;
    ViewBag.variable = Model.GetHashCode().ToString();
}


@*Entregadas 	Recogidas 	Observación 	Observaciones*@

<tr id="@(ViewBag.variable)">
    <td></td>
    <td>@Html.DropDownListFor(x => x.recogidasR.IDCLIENTE, new SelectList(Model.listaClientes, "ID", "NOMBREC"), new { @class = "form-control" })</td>
    <td>@Html.EditorFor(x => x.recogidasR.GRADDEJA, new { htmlAttributes = new { @class = "form-control" } })</td>
    <td>@Html.EditorFor(x => x.recogidasR.GRADRECOGE, new { htmlAttributes = new { @class = "form-control" } })</td>
    <td>@Html.DropDownListFor(x => x.recogidasR.IDOBSERVA, new SelectList(Model.listaObservaciones, "ID", "OBSERVA"), new { @class = "form-control" })</td>
    <td>@Html.EditorFor(x => x.recogidasR.OBSERVACIONES, new { htmlAttributes = new { @class = "form-control" } })</td>
    <td><a id="deleteFilaContent_ajax"><span class="glyphicon glyphicon-remove" aria-hidden="true"></span></a></td>
</tr>